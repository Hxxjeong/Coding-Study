package baekjoon.bronze;

import java.io.*;
import java.util.*;

public class b251003_1157 {	// 가장 많이 사용된 문자 대문자로 출력하기
	public static void main(String[] args) throws IOException {
		// 빠른 입력을 위해 BufferedReader 사용
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String input = br.readLine().toUpperCase(); // 대소문자 구분 제거
		
		// 알파벳 개수 저장할 배열 (A~Z → 26개) 
        int[] alphabet = new int[26];
        
        // 알파벳 다 배열을 만들어서 input에서 알파벳 개수만큼 그 배열에서 개수 올리기
        for(int i=0;i<input.length();i++) {
        	char ch = input.charAt(i);
        	// 알파벳 문자를 0부터 시작하는 인덱스 숫자로 바꾸기
        	// A는0번째, B는 1번째... 로 바꾸는 방법 - 'A'는 65
        	alphabet[ch-'A']++;
        }
        
        // 제일 많이 나온 값 -1로 초기화(0번 나와도 max보다 무조건 커서 저장되므로) -안정성
        // -1을 초기값으로 쓰면 "아직 값이 없다"는 명확한 의미
        int max = -1;
        char result='?';
        
        for(int i=0;i<26;i++) {
        	if(alphabet[i]>max) {
        		max=alphabet[i];
        		// 다시 원래대로 코드값 갖게한 후 문자열로 변환
        		result=(char)(i+'A');
        	} else if(alphabet[i]==max) {
        		result='?';
        	}
        }
        
        System.out.println(result);
	}
}
