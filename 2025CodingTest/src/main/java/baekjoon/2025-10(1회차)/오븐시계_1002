package baekjoon.bronze;

import java.io.*;
import java.util.StringTokenizer;

public class b251002_2525 {	// 오븐시계 - 시간 더하기
	public static void main(String[] args) throws IOException {
		// 빠른 입력을 위해 BufferedReader 사용
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		// 첫 줄에서 현재 시각 값을 입력받음 (예: "14 30")
        StringTokenizer st = new StringTokenizer(br.readLine());
        int hour = Integer.parseInt(st.nextToken());
        int minute = Integer.parseInt(st.nextToken());

        // 둘째 줄 입력 요리시간 (15)
        int cookTime = Integer.parseInt(br.readLine());
        
        // 전체 분 계산 - 시간을 계산할 때는 전체 분으로 바꿔서 계산하면 쉬움-외우기
        int totalMinutes = hour*60+minute+cookTime;
        
        int newHour = (totalMinutes / 60) % 24;  // 24시간 이상이면 0시부터 다시
        int newMinute = totalMinutes % 60;

        System.out.println(newHour + " " + newMinute);
		
	}
}
